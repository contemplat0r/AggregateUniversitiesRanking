        if (a.options.displayCounter && a.gallery.length > 1) {
            var k = a.gallery.length;
            if (a.options.counterType == "skip") {
                var m = 0,
                    n = k,
                    o = parseInt(a.options.counterLimit) || 0;
                if (o < k && o > 2) {
                    var p = Math.floor(o / 2);
                    m = a.current - p;
                    if (m < 0) m += k;
                    n = a.current + (o - p);
                    if (n > k) n -= k
                }
                while (m != n) {
                    if (m == k) m = 0;
                    l += '<a onclick="Shadowbox.change(' + m + ');"';
