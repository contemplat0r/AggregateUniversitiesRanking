        if (a.options.displayNav) {
            e = true;
            var k = a.gallery.length;
            if (k > 1) if (a.options.continuous) f = i = true;
                else {
                    f = k - 1 > a.current;
                    i = a.current > 0
                }
            if (a.options.slideshowDelay > 0 && a.hasNext()) {
                h = !a.isPaused();
                g = !h
            }
        } else e = f = g = h = i = false;
